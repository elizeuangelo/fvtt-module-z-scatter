{
    "compilerOptions": {
        "skipLibCheck": true,
        "types": [
            "@league-of-foundry-developers/foundry-vtt-types"
        ],
        "lib": [
            "ES2022",
            "DOM",
            "DOM.Iterable"
        ],
        "target": "ES2022",
        "module": "ES2022",
        //"strict": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "removeComments": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
    },
}